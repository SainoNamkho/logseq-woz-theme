{
  "name": "logseq-woz-theme",
  "author": "Hayden",
  "version": "0.0.7",
  "description": "a theme base logseq-dev-theme",
  "logseq": {
    "themes": [
      {
        "name": "Woz Theme",
        "url": "./custom.css",
        "description": "a theme base logseq-dev-theme"
      }
    ],
    "id": "hayden_woz_theme",
    "icon": "./logo.png"
  },
  "scripts": {
    "build": "yarn sass src/main.scss dist/custom.css --no-source-map && cp package.json logo.png dist && cp -ri src/assets/ dist/assets"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "sass": "^1.35.1"
  }
}
